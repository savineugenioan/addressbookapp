{
  "ConnectionStrings": {
    //The "DefaultConnection" string will be replaced if docker-compose is utilized.
    "DefaultConnection": "Data Source=DESKTOP-AQUHL31;Initial Catalog=AddressBookDemo;Integrated Security=True;Persist Security Info=False;Pooling=False;MultipleActiveResultSets=False;Connect Timeout=60;Encrypt=False;TrustServerCertificate=False",
    "NumberOfPersonsToSeed": "100"
  },
  "Serilog": {
    "Properties": {
      "ApplicationName": "AddressBookApp"
    },
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId", "CorrelationId" ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
